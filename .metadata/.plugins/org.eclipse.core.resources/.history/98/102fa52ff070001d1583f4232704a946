package threads;

import java.util.concurrent.Semaphore;

import prodcons.IProdConsBuffer;

public class Producteur extends Thread{
	Semaphore mutex;
	IProdConsBuffer buffer;
	public Producteur(Semaphore s, IProdConsBuffer b){
		mutex = s;
		buffer = b;
		this.start();
	}
	
	public void run() {
		
	}
}
